{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run start",
    "frontendDist": "../dist/smart-energy-explorer-angular/browser",
    "devUrl": "http://localhost:4200"
  },
  "bundle": {
    "active": true,
    "category": "Utilities",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null,
      "files": {
        "embedded.provisionprofile": null
      }
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "createUpdaterArtifacts": false
  },
  "productName": "smart-energy-explorer",
  "version": "../package.json",
  "identifier": "io.github.rars.smart-energy-explorer",
  "plugins": {},
  "app": {
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 768,
        "resizable": true,
        "title": "Smart Energy Explorer",
        "width": 1024,
        "visible": false
      },
      {
        "label": "splashscreen",
        "url": "/welcome",
        "title": "Smart Energy Explorer",
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    }
  }
}
